<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema file:/C:/Program%20Files%20(x86)/Exchanger%20XML%20Editor%203.3/types/XML%20Schema/Validation/XMLSchema.xsd">
	<xsd:element name="Lunch">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="starter">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="juicer"></xsd:enumeration>
							<xsd:enumeration value="soup"></xsd:enumeration>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="maincourse">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="veg"></xsd:enumeration>
							<xsd:enumeration value="nonveg"></xsd:enumeration>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="dessert">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="icecream"></xsd:enumeration>
							<xsd:enumeration value="fruitsalad"></xsd:enumeration>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>	
	
</xsd:schema>